{
    "compilerOptions": {
      "allowJs": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,  
      "target": "es2022",
      "module":"preserve",
      "moduleResolution":"node",
      "noEmit": true,
      "outDir": "build",
      "preserveConstEnums": true,
      "skipLibCheck": true,
      "sourceMap": false,
      "strict": true,
    },
    "exclude": ["node_modules", "**/*.test.ts"]
  }